{
    "name": "react-parallel/contracts",
    "description": "Interfaces for ReactPHP ext-parallel related packages",
    "license": "MIT",
    "authors": [
        {
            "name": "Cees-Jan Kiewiet",
            "email": "ceesjank@gmail.com",
            "homepage": "https://wyrihaximus.net/"
        }
    ],
    "require": {
        "php": "^8.2",
        "react/promise": "^3",
        "wyrihaximus/pool-info": "^2"
    },
    "require-dev": {
        "wyrihaximus/async-test-utilities": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "ReactParallel\\Contracts\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ReactParallel\\Tests\\Contracts\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "8.2.13"
        },
        "sort-packages": true,
        "allow-plugins": {
            "infection/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "icanhazstring/composer-unused": true,
            "ergebnis/composer-normalize": true
        }
    }
}
